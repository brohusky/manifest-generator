{
  "name": "manifest-generator",
  "version": "0.1.0",
  "description": "generate.brohusky.com will provide users with an interface to generate manifest file.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr"
  ],
  "author": "Mahan hazrati",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brohusky/manifest-generator.git"
  },
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "tslint './src/**/*.ts*' --format stylish --project . --force",
    "start": "yarn run start-dev",
    "dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "prod": "yarn run build && node start.js",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "express": "^4.17.1",
    "file-loader": "^4.1.0",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.10",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "image-webpack-loader": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {}
}
